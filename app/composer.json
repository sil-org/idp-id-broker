{
  "name": "sil-org/idp-id-broker",
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "codemix/yii2-streamlog": "^1.3",
    "google/apiclient": "^2.4",
    "sil-org/php-array-dot-notation": "0.1.0",
    "sil-org/php-env": "^2.1.1 || ^3.0",
    "yiisoft/yii2": "^2.0.11",
    "yiisoft/yii2-gii": "*",
    "sil-org/psr3-adapters": "^3.1.0",
    "sil-org/yii2-json-log-targets": "^2.0",
    "br33f/php-ga4-mp": "^0.1.0",
    "yiisoft/yii2-swiftmailer": "^2.0",
    "ramsey/uuid": "^3.6",
    "guzzlehttp/guzzle": "^6.2",
    "webmozart/assert": "^1.2",
    "sil-org/yii2-sentry": "^2.0",
    "aws/aws-sdk-php": "~3.288.1",
    "ext-pdo": "*"
  },
  "require-dev": {
    "behat/behat": "^3.3",
    "roave/security-advisories": "dev-master",
    "friendsofphp/php-cs-fixer": "^3.0",
    "spomky-labs/otphp": "^11.3",
    "psr/clock": "^1.0"
  },
  "config": {
    "process-timeout": 1800,
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "php-http/discovery": false
    }
  },
  "autoload": {
    "psr-4": {
      "Sil\\SilIdBroker\\Behat\\Context\\": "features/bootstrap"
    }
  }
}
